<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.treevalue.robot.resource.pgsql.mapper.ResourceFeedbackEvaluationMapper">
       <select id="findEvaluationByResourceId" resultType="java.lang.Double">
           select evaluation from tvr_resource_feedback_evaluation where resourceId=#{resourceId}
    </select>
</mapper>
